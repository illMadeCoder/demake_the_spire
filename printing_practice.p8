version 32
__lua__
function print_cc(_cc_str, 
																		_x, 
																		_y, 
																		_dc)
	local c = nil
	local state = 0
	local print_length = 0
	for i=1, #_cc_str do
		local char = sub(_cc_str, i, i)
		if state == 2 and 
					char == "$" then
			c = nil
			state = 0
		elseif state == 1 then
			c = char
			state = 2
		elseif state == 0 and 
									char == "$" then
		 state = 1		 
		else
			print(char, _x+print_length*4, _y, c or _dc)	
			print_length += 1
		end
	end
end

function _draw()
end
